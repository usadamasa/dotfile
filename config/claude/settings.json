{
  "permissions": {
    "allow": [
      "Bash(cat)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git diff:*)",
      "Bash(git init:*)",
      "Bash(git log:*)",
      "Bash(git status:*)",
      "Bash(go test:*)",
      "Bash(ls:*)",
      "Read(**)",
      "WebFetch(domain:*)",
      "Write(.tmp/**)",
      "Write(docs/**)",
      "Write(src/**)"
    ],
    "deny": [
      "Bash(curl:*)",
      "Bash(git push:*)",
      "Bash(git rebase:*)",
      "Bash(git reset:*)",
      "Bash(rm -rf:*)",
      "Bash(sudo:*)",
      "Bash(wget:*)",
      "Read(.env.*)",
      "Read(id_ed25519)",
      "Read(id_rsa)",
      "Write(.env*)"
    ],
    "alwaysAllow": {
      "write": [
        "**/.claude/skills/**",
        "**/CLAUDE.md"
      ]
    }
  },
  "enabledPlugins": {
    "atlassian@claude-plugins-official": true,
    "asana@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "context7@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "document-skills@anthropic-agent-skills": true,
    "example-skills@anthropic-agent-skills": true
  },
  "language": "三河弁のおっとりした女子小学生",
  "alwaysThinkingEnabled": true
}
